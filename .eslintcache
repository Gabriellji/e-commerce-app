ccount) => account.user === this.state.username\r\n    );\r\n    console.log(account)\r\n    account.length === 0\r\n      ? this.setState({ loginFailed: true, username: \"\", password: \"\" })\r\n      : account[0].password === this.state.password\r\n      ? this.setState({\r\n          isLoggedIn: true,\r\n          loginIconClicked: false,\r\n          loginFailer: false,\r\n          user: account[0].name,\r\n          username: \"\",\r\n          password: \"\",\r\n        })\r\n      : this.setState({ loginFailed: true });\r\n  };\r\n  logout = () =>\r\n    this.setState({\r\n      isLoggedIn: !this.state.isLoggedIn,\r\n      loginIconClicked: !this.state.loginIconClicked,\r\n    });\r\n\r\n  /*ADD STUFF TO CART LOGIC HERE*/\r\n  /*addedItems:[] is already in state*/\r\n\r\n  /*SHOP PRICE LOGIC HERE*/\r\n  /*itemsPrice inside state and hardcoded*/\r\n\r\n  render() {\r\n    return (\r\n      <ShopContext.Provider\r\n        value={{\r\n          state: this.state,\r\n          toggleMobileMenu: this.toggleMobileMenu,\r\n          loginIconToggle: this.loginIconToggle,\r\n          login: this.login,\r\n          logout: this.logout,\r\n          username: this.username,\r\n          password: this.password,\r\n          closeLogin: this.closeLogin,\r\n          cartToggle: this.cartToggle,\r\n        }}\r\n      >\r\n        {this.props.children}\r\n      </ShopContext.Provider>\r\n    );\r\n  }\r\n}\r\n\r\nexport default ShopProvider;\r\n","D:\\ROOT\\e-commerce-app\\src\\context\\APIContext.js",[],"D:\\ROOT\\e-commerce-app\\src\\App.js",["276","277","278","279","280"],"import React, { useContext, useState } from \"react\";\r\nimport { Switch, Route/* , BrowserRouter as Router  */} from \"react-router-dom\";\r\n\r\nimport Home from \"./components/pages/home\";\r\nimport Shop from \"./components/pages/shop\";\r\nimport Map from \"./components/pages/map\";\r\nimport Contact from \"./components/pages/contact\";\r\nimport { ShopContext } from \"./context/ShopContext\";\r\n\r\nconst App = () => {\r\n  const [shop, setShop] = useState({\r\n    title: \"This is shop component\",\r\n  });\r\n\r\n  const [map, setMap] = useState({\r\n    title: \"This is map component\",\r\n  });\r\n\r\n  const [contact, setContact] = useState({\r\n  });\r\n\r\n/*  const value = useContext(Router) */ /* how to use context with hooks. wtf jane <3 */\r\n\r\n  return (\r\n        <Switch>\r\n          <Route exact path=\"/\" render={() => <Home />} />\r\n          <Route path=\"/home\" render={() => <Home />} />\r\n\r\n          <Route\r\n            path=\"/shop\"\r\n            render={(props) => <Shop {...props} title={shop.title} />}\r\n          />\r\n          <Route\r\n            path=\"/map\"\r\n            render={(props) => <Map {...props} title={map.title} />}\r\n          />\r\n          <Route\r\n            path=\"/contact\"\r\n            render={(props) => <Contact {...props} title={contact.title} />}\r\n          />\r\n        </Switch>\r\n  );\r\n};\r\n\r\nexport default App;\r\n","D:\\ROOT\\e-commerce-app\\src\\data\\items.js",[],"D:\\ROOT\\e-commerce-app\\src\\data\\loginData.js",[],"D:\\ROOT\\e-commerce-app\\src\\data\\textData.js",[],"D:\\ROOT\\e-commerce-app\\src\\styles\\styles.js",["281"],"import { Children } from \"react\";\r\nimport { NavLink } from \"react-router-dom\";\r\nimport styled from \"styled-components\";\r\n\r\n/*\r\n========================\r\nAtoms Components\r\n========================\r\n*/\r\n// Hr\r\n\r\nexport const StyledHr = styled.hr`\r\n  border-top: 2px solid\r\n    ${(p) => (p.color === \"light\" ? \"var(--light)\" : \"var(--primary)\")};\r\n  margin: var(--spacer-8);\r\n\r\n  /* @media (max-width: 768px) {\r\n    display: none;\r\n  } */\r\n`;\r\n\r\n// Texts\r\nexport const StyledText = styled.p`\r\n  color: ${(p) =>\r\n    p.color === \"primary\"\r\n      ? \"var(--primary)\"\r\n      : p.color === \"light\"\r\n      ? \"var(--light)\"\r\n      : \"var(--dark)\"};\r\n  font-size: ${(p) =>\r\n   