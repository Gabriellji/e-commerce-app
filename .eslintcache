n                    <CartItem\n                        key={i}\n                        name={name.split(\" \").slice(0, 3).join(\" \")}\n                        image={imageS}\n                        price={`Price ${price}`}\n                        increase\n                        decrease\n                        remove={onRemoveClick}\n                        quantity={findQuontity(context.cart, id).length}\n                        id={id}\n                    />\n                );\n            });\n    };\n\n    const clearAllItems = () => {\n        context.setCart('');\n    }\n\n    const getTotalPrice = () => {\n        if (!context.cart) {\n            return 0;\n        }\n        let arr = [];\n        context.cart.map((el) => arr.push(el.price));\n        const result = arr.reduce((acc, val) => acc + val, 0);\n        return result.toFixed(2);\n    };\n\n    return (\n        <div>\n            <div\n                className=\"cart_wrap\"\n                onClick={toggleModal}\n            >\n                <Text\n                    color={textColor}\n                    size=\"S\"\n                    text={`${getTotalPrice()} £`}\n                />\n                <SuperNavImg src={cart} alt=\"cart\" />\n                <TextCart\n                    size=\"S\"\n                    color={textColor}\n                    text={context.cart && context.cart.length}\n                />\n            </div>\n\n\n            <Modal\n                isOpen={isOpen}\n                onRequestClose={toggleModal}\n                contentLabel=\"shopping-cart\"\n                className=\"mymodal\"\n                overlayClassName=\"myoverlay\"\n                closeTimeoutMS={500}\n            >\n                {!context.cart.length ? (\n                    <h4>You havent added any items YET</h4>\n                ) : (\n                        renderAddedItems(context.cart)\n                    )}\n                <Button\n                    size=\"S\"\n                    color=\"primary\"\n                    action={toggleModal}\n                    text=\"Continue shopping\"\n                />\n                <Button\n                    size=\"S\"\n                    color=\"primary\"\n                    text=\"clear\"\n                    action={clearAllItems}\n                />\n                <p>\n                    Total Items\n                </p>\n                <p>\n                    Total Payment £ {getTotalPrice()}\n                </p>\n                <p>\n                    <StripeCheckoutButton\n                        price={getTotalPrice()} />\n                </p>\n            </Modal>\n        </div>\n    );\n\n}\n\nexport default Cart;","/Users/gabriellji/Documents/projects/e-commerce-app/src/components/pages/shop/shop-icon/ShopIcon.js",[],"/Users/gabriellji/Documents/projects/e-commerce-app/src/components/pages/map/mapbox/tab/Tab.js",[],"/Users/gabriellji/Documents/projects/e-commerce-app/src/components/pages/map/mapbox/popup/Popup.js",[],"/Users/gabriellji/Documents/projects/e-commerce-app/src/components/atoms/text/TextCart.js",[],"/Users/gabriellji/Documents/projects/e-commerce-app/src/components/pages/shop/cart-item/index.js",[],"/Users/gabriellji/Documents/projects/e-commerce-app/src/components/pages/shop/cart-item/CartItem.js",[],{"ruleId":"301","replacedBy":"302"},{"ruleId":"303","replacedBy":"304"},{"ruleId":"305","severity":1,"message":"306","line":1,"column":17,"nodeType":"307","messageId":"308","endLine":1,"endColumn":27},{"ruleId":"305","severity":1,"message":"309","line":8,"column":10,"nodeType":"307","messageId":"308","endLine":8,"endColumn":21},{"ruleId":"305","severity":1,"message":"310","line":11,"column":16,"nodeType":"307","messageId":"308","endLine":11,"endColumn":23},{"ruleId":"305","severity":1,"message":"311","line":15,"column":15,"nodeType":"307","messageId":"308","endLine":15,"endColumn":21},{"ruleId":"305","severity":1,"message":"312","line":19,"colum