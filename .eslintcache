</Route>\n\n          {value.state.items.map((category, categoryIndex) =>\n            category.shops.map((shops, shopsIndex) => {\n              let link = `/shop/category${categoryIndex + 1}/${shopsIndex}`;\n              return (\n                <Route exact path={link}>\n                  <SingleShop category={categoryIndex + 1} shop={shopsIndex} />\n                </Route>\n              );\n            })\n          )}\n\n          <Route\n            render={() => {\n              const regExp = /\\d+\\/\\d+\\/\\d+/;\n              let res = props.location.pathname\n                .match(regExp)[0]\n                .split(\"/\")\n                .map((el) => Number(el));\n              const category = res[0] - 1;\n              const shop = res[1];\n              const index = res[2];\n              return (\n                <> \n                  <SingleShopCard\n                    name={value.state.items[category].shops[shop][index].name}\n                    key={props.index}\n                    price={value.state.items[category].shops[shop][index].price}\n                  />\n                  <Link to={`/shop/category${category + 1}/${shop}`}>BACK</Link>\n                </>\n              );\n            }}\n          />\n        </Switch>\n        </>\n      )}\n      </>\n    }\n  </MyAPIContext.Consumer>\n);\n\nexport default Shop;\n","C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\home\\Home.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\shop\\remote-shop\\SingleShop.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\shop\\remote-shop\\SingleShopCard.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\data\\textData.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\atoms\\SuperNav\\SuperNav.js",[],"C:\\Users\\geom