\n\r\n  const onIncrease = (e) => {\r\n    let element = findItem(context.cart, Number(e.target.id));\r\n    let copyOfItems = [...context.cart];\r\n    copyOfItems.push(element);\r\n    context.setCart(copyOfItems);\r\n  }\r\n\r\n  const onRemoveClick = (e) => {\r\n    let element = findItem(context.cart, Number(e.target.id));\r\n    const index = context.cart.indexOf(element);\r\n\r\n    let copyOfItems = [...context.cart];\r\n    copyOfItems.splice(index, 1);\r\n    context.setCart(copyOfItems);\r\n  };\r\n\r\n  const renderAddedItems = (arr) => {\r\n    return arr\r\n      .filter((value, index, self) => self.indexOf(value) === index)\r\n      .map(({ id, name, price, imageS }, i) => {\r\n        return (\r\n          <CartItem\r\n            key={i}\r\n            name={name.split(\" \").slice(0, 3).join(\" \")}\r\n            image={imageS}\r\n            price={`Price ${price}`}\r\n            increase={onIncrease}\r\n            decrease={onRemoveClick}\r\n            remove={onRemoveClick}\r\n            quantity={findQuontity(context.cart, id).length}\r\n            id={id}\r\n          />\r\n        );\r\n      });\r\n  };\r\n\r\n  const clearAllItems = () => {\r\n    context.setCart(\"\");\r\n  };\r\n\r\n  const getTotalPrice = () => {\r\n    if (!context.cart) {\r\n      return 0;\r\n    }\r\n    let arr = [];\r\n    context.cart.map((el) => arr.push(el.price));\r\n    const result = arr.reduce((acc, val) => acc + val, 0);\r\n    return result.toFixed(2);\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      <StyledCart onClick={toggleModal}>\r\n        {type !== \"mobile\" && (\r\n          <Text color={textColor} size=\"S\" text={`${getTotalPrice()}£`} />\r\n        )}\r\n        <SuperNavImg src={cart} alt=\"cart\" />\r\n        {context.cart.length > 0 && (\r\n          <TextCart\r\n            size=\"S\"\r\n            color={textColor}\r\n            text={context.cart && context.cart.length}\r\n          />\r\n        )}\r\n      </StyledCart>\r\n\r\n      <Modal\r\n        isOpen={isOpen}\r\n        onRequestClose={toggleModal}\r\n        contentLabel=\"shopping-cart\"\r\n        className=\"mymodal\"\r\n        overlayClassName=\"myoverlay\"\r\n        closeTimeoutMS={500}\r\n      >\r\n        {!context.cart.length ? (\r\n          <h4>You havent added any items YET</h4>\r\n        ) : (\r\n          renderAddedItems(context.cart)\r\n        )}\r\n        <Button\r\n          size=\"S\"\r\n          color=\"primary\"\r\n          action={toggleModal}\r\n          text=\"Continue shopping\"\r\n        />\r\n        <Button size=\"S\" color=\"primary\" text=\"clear\" action={clearAllItems} />\r\n        <p>Total Items</p>\r\n        <p>Total Payment £ {getTotalPrice()}</p>\r\n        <p>\r\n          <StripeCheckoutButton price={getTotalPrice()} />\r\n        </p>\r\n      </Modal>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Cart;\r\n","D:\\ROOT\\e-commerce-app\\src\\components\\pages\\shop\\cart-item\\index.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\pages\\shop\\cart-item\\CartItem.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\pages\\home\\HomeButtons.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\atoms\\footer\\Footer.js",["316","317","318","319","320","321","322","323","324","325","326","327"],"D:\\ROOT\\e-commerce-app\\src\\utils\\refreshToken.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\button\\Button.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\text\\TextCart.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\text\\Text.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\atoms\\mainNavbar\\SuperNav\\SuperNav.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\button\\ButtonX.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\input\\LoginInput.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\hr\\Hr.js",[],"D:\\ROOT\\e-commerce-app\\src\\components\\subatoms\\images\\HomeImg.js",[],"D:\\ROOT\\e-commerce-app\\src\\c