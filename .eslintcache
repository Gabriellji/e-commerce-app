sh(item)\n    );\n    context.setCart(copyOfItems);\n  };\n\n  const renderAddedItems = (arr) => {\n    return arr\n      .filter((value, index, self) => self.indexOf(value) === index)\n      .map(({ id, name, price, imageL }, i) => {\n        return (\n          <CartItem\n            key={i}\n            name={name.split(\" \").slice(0, 3).join(\" \")}\n            image={imageL}\n            price={`Price ${price}`}\n            increase={onIncrease}\n            decrease={onRemoveClick}\n            remove={deleteItem}\n            quantity={findQuantity(context.cart, id).length}\n            id={id}\n          />\n        );\n      });\n  };\n\n  const clearAllItems = () => {\n    context.setCart(\"\");\n  };\n\n  const getTotalPrice = () => {\n    if (!context.cart) {\n      return 0;\n    }\n    let arr = [];\n    context.cart.map((el) => arr.push(el.price));\n    const result = arr.reduce((acc, val) => acc + val, 0);\n    return result.toFixed(2);\n  };\n\n  return (\n    <>\n      <StyledCart onClick={toggleModal}>\n        {type !== \"mobile\" && (\n          <Text color={textColor} size=\"S\" text={`${getTotalPrice()}£`} />\n        )}\n        <SuperNavImg src={cart} alt=\"cart\" />\n        {context.cart.length > 0 && (\n          <TextCart\n            size=\"S\"\n            color={textColor}\n            text={context.cart && context.cart.length}\n          />\n        )}\n      </StyledCart>\n      <Modal\n        isOpen={isOpen}\n        onRequestClose={toggleModal}\n        contentLabel=\"shopping-cart\"\n        className=\"mymodal\"\n        overlayClassName=\"myoverlay\"\n        closeTimeoutMS={500}\n      >\n        {!context.cart.length ? (\n          <Text\n            size=\"L\"\n            color=\"primary\"\n            text={textData.shop.cart.empty}\n            align=\"center\"\n          />\n        ) : (\n          <StyledCartGridSection>\n            {renderAddedItems(context.cart)}\n          </StyledCartGridSection>\n        )}\n        <Hr />\n        <StyledCartLastSection>\n          <Button\n            size=\"M\"\n            color=\"primary\"\n            action={toggleModal}\n            text={textData.shop.cart.exit}\n            width=\"parent\"\n          />\n          <Button\n            size=\"M\"\n            color=\"primary\"\n            text={textData.shop.cart.clear}\n            action={clearAllItems}\n            width=\"parent\"\n          />\n          <div>\n            <Text size=\"M\" color=\"dark\" text={textData.shop.cart.items} />\n            <Text size=\"M\" color=\"primary\" text={context.cart.length} />\n          </div>\n          <div>\n            <Text size=\"M\" color=\"dark\" text={textData.shop.cart.price} />\n            <Text size=\"M\" color=\"primary\" text={`${getTotalPrice()} £`} />\n          </div>\n        </StyledCartLastSection>\n        <StripeCheckoutButton price={getTotalPrice()} />\n      </Modal>\n    </>\n  );\n};\n\nexport default Cart;\n","C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\atoms\\mainNavbar\\navbar\\HoverMenuContent.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\utils\\refreshToken.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\subatoms\\text\\TextCart.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\pages\\shop\\cart-item\\CartItem.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\pages\\shop\\stripe-button\\index.js",[],"C:\\Users\\geomj\\OneDrive\\Documenti\\Wilder\\00Project2\\e-commerce-app\\src\\components\\atoms\\footer\\FooterHeader.js",[],{"ruleId":"317","replacedBy":"318"},{"ruleId":"319","replacedBy":"320"},{"ruleId":"321","severity":1,"message":"322","line":9,"column":16,"nodeType":"323","messageId":"324","endLine":9,"endColumn":23},{"ruleId":"321","severity":1,"message":"3